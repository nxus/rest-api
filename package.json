{
  "name": "@nxus/rest-api",
  "version": "2.4.0-1",
  "description": "REST API for Nxus applications",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R spec",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepublish": "npm run compile",
    "postpublish": "npm run docs-readme && npm run commit-update-docs",
    "docs-readme": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/*.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "commit-update-docs": "git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nxus/rest-api.git"
  },
  "keywords": [
    "nxus",
    "admin",
    "crud"
  ],
  "author": "mjreich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nxus/rest-api/issues"
  },
  "homepage": "https://github.com/nxus/rest-api",
  "dependencies": {
    "babel-runtime": "^5.7.0"
  },
  "peerDependencies": {
    "@nxus/core": "^2.4.0-0",
    "@nxus/router": "^2.4.0-0",
    "@nxus/storage": "^2.4.0-0"
  },
  "devDependencies": {
    "@nxus/core": "^2.4.0-0",
    "babel": "^5.7.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.2.5",
    "sinon": "^1.17.2",
    "documentation": "^4.0.0-beta"
  }
}
